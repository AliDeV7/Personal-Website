<!-- Education -->
<h2 class="title title--h2"><span class="box icon-box"><i class="font-icon feathericon-book-open"></i></span>@Resources.Lable.Education</h2>
<div class="timeline">
    <!-- Item -->
    <article class="timeline__item">
        <h5 class="title title--h5 timeline__title">@Resources.Data.University</h5>
        <span class="timeline__period">@Resources.Data.University_Year</span>
        <p class="timeline__description">@Resources.Data.University_Des </p>
    </article>

    <!-- Item -->
    <article class="timeline__item">
        <h5 class="title title--h5 timeline__title">@Resources.Data.SIMI</h5>
        <span class="timeline__period">@Resources.Data.SIMI_Year</span>
        <p class="timeline__description">@Resources.Data.SIMI_Des </p>
    </article>

    <!-- Item -->
    <article class="timeline__item">
        <h5 class="title title--h5 timeline__title">@Resources.Data.SIMI</h5>
        <span class="timeline__period">@Resources.Data.SIMI_Year1</span>
        <p class="timeline__description">@Resources.Data.SIMI_Des1 </p>
    </article>

        <!-- Item -->
    <article class="timeline__item">
